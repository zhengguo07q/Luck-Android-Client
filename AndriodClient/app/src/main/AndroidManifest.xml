<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goleee.luck">

    <application
        android:name=".LuckApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WelcomeProtocolActivity" />
        <activity android:name=".activity.LoginPhoneActivity" />
        <activity android:name=".activity.LoginPhoneVerifyActivity" />
        <activity android:name=".activity.LoginPasswordVerifyActivity" />
        <activity
            android:name=".activity.RegisterBaseInfoActivity"
            android:label="@string/title_activity_register_base_info"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.RegisterHeadshotActivity"></activity>
    </application>

</manifest>